@using System.Globalization
@model List<MagazineInfo>
@{
    ViewData["Title"] = "Home Page";
    Layout = "~/Views/Shared/_Layout.cshtml";

}

@{
    // var magazine =ViewBag.lastmagazine as List<Magazine>;
